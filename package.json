{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "redesigned-waffle",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test-coverage": "vitest run --coverage",
    "format": "biome format src lib --write",
    "lint:ts": "tsc",
    "lint:eslint": "eslint src lib --cache",
    "lint:biome": "biome check src lib",
    "lint:biome-ci": "biome ci src lib",
    "build:page": "vite build",
    "build:lib": "vite build --mode lib",
    "build:lib-type": "tsc --project ./tsconfig.lib.json",
    "serve": "vite",
    "watch:test": "vitest watch",
    "watch:ts": "tsc --watch",
    "check": "run-s format lint:ts lint:eslint lint:biome test build:page build:lib build:lib-type"
  },
  "keywords": [],
  "author": "mikiymk",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikiymk/redesigned-waffle.git"
  },
  "license": "MIT",
  "dependencies": {
    "@biomejs/biome": "^1.7.3",
    "@vanilla-extract/css": "^1.15.2",
    "solid-js": "^1.8.17"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vanilla-extract/vite-plugin": "^4.0.10",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^1.6.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.3.0",
